<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="dir" default="/home/dcist/bags/falcon_forest"/>
  <node pkg="rosbag" type="record" name="recorder" args="
  -o $(arg dir)
  /quadrotor/odom
  /quadrotor/vio/odom
  /os_node/imu_packets
  /os_node/lidar_packets
  /ovc/left/camera_info
  /ovc/left/image_raw
  /ovc/right/camera_info
  /ovc/right/image_raw   
  /ovc/vectornav/imu
  /ovc/vectornav/mag
  /tf_static
  "
  output="screen"/>
</launch>
